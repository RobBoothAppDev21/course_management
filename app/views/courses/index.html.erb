<h1>Courses#index</h1>
<p>Find me in app/views/courses/index.html.erb</p>

<table>
  <tr>
    <th>
      Title
    </th>
    <th>
      Course Number
    </th>
    <th>
      Sections
    </th>
    <th>
      Credits
    </th>
  </tr>

  <% @courses.each do |course| %>
    <tr>
      <td>
        <%= course.title%>
      </td>
      <td>
        <%= course.number%>
      </td>
      <td>
        <%= course.sections.size%>
      </td>
      <td>
        <%= course.sections.first.credits%>
      </td>
    </tr>
  <% end %>
</table>